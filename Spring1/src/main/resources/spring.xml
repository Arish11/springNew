<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
        <bean id="doc" class="dependencyDemo.Doctor">
        <property name="experience" value="12 years"></property>
        <property name="hospital" ref="hospital"></property>
        </bean>
        <bean id="nurse" class="dependencyDemo.Nurse"></bean>
        <bean id="hospital" class="dependencyDemo.Hospital"></bean>
        
</beans>